{
    "version": 2,
    "builds": [
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
           
        },
        {
            "src": "shortee/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "distDir": "staticfiles",
                "runtime": "python3.10",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
       
        {
            "src": "/(.*)",
            "dest": "shortee/wsgi.py"
        }
    ]
} 